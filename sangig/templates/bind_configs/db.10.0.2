;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.sangig. root.localhost.sangig. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns1.sangig.
101	IN	PTR	ns1.sangig.
101	IN	PTR	server.sangig.
2	IN	PTR	kodirovka.sangig.
1	IN	PTR	vpnrouter.sangig.
3	IN	PTR	sangig06.sangig.
