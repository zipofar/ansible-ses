;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.ooi. root.localhost.ooi. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns1.ooi.
101	IN	PTR	ns1.ooi.
101	IN	PTR	server.ooi.
1	IN	PTR	vpnrouter.ooi.
100	IN	PTR	internetrouter.ooi.
